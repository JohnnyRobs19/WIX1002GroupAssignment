<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewinfo.EMPLOYEEController">
   <children>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="56.0" prefWidth="231.0">
         <children>
            <Label text="EMPLOYEE">
               <font>
                  <Font size="43.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox layoutX="693.0" layoutY="549.0" prefHeight="37.0" prefWidth="93.0">
         <children>
            <Button mnemonicParsing="false" onAction="#employeeBackButtonPushed" prefHeight="20.0" prefWidth="80.0" text="Back" />
         </children>
      </HBox>
      <HBox layoutX="24.0" layoutY="203.0" prefHeight="315.0" prefWidth="744.0">
         <children>
            <TableView fx:id="employeeTableView" prefHeight="423.0" prefWidth="751.0">
              <columns>
                <TableColumn fx:id="employeeID" prefWidth="193.0" text="EMPLOYEE ID" />
                <TableColumn fx:id="employeeName" prefWidth="201.0" text="EMPLOYEE NAME" />
                  <TableColumn fx:id="employeeStatus" prefWidth="180.0" text="STATUS" />
                  <TableColumn fx:id="employeePassword" prefWidth="169.0" text="PASSWORD" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox layoutX="28.0" layoutY="124.0" prefHeight="48.0" prefWidth="744.0" spacing="10.0">
         <children>
            <Label text="ID:">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <TextField fx:id="employeeIdFilterField" prefHeight="31.0" prefWidth="46.0" />
            <Label text="NAME:">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <TextField fx:id="employeeNameFilterField" prefHeight="31.0" prefWidth="68.0" />
            <Label text="STATUS">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="employeeStatusFilterField" prefHeight="31.0" prefWidth="101.0" />
            <Label text="PASSWORD">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <TextField fx:id="employeePasswordFilterField" prefHeight="31.0" prefWidth="118.0" />
            <Button mnemonicParsing="false" onAction="#searchButtonPushed" text="SEARCH" />
         </children>
      </HBox>
      <Button layoutX="684.0" layoutY="78.0" mnemonicParsing="false" onAction="#resetButtonPushed" prefHeight="31.0" prefWidth="64.0" text="RESET" />
   </children>
</AnchorPane>
