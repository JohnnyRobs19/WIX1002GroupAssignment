<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1133.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewinfo.SALESController">
   <children>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="328.0" prefWidth="535.0">
